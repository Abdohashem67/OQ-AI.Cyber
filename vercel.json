{
  "version": 2,
  "functions": {
    "api/main.py": {
      "maxDuration": 30
    }
  },
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/main.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production"
  }
}
